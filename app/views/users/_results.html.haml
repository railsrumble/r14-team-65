%section.results
  %h3 Results:

  - user.gists.each do |gist|
    %div.result
      %h4= gist.gist_files.first.name
      .small
        created today
      .tags
        %input.gist-input-tags(type="text" value="#{gist.tag_list}")


        %span.tag.label.label-default
          ruby
        %span.tag.label.label-default
          railsrumble
        %span.tag.label.label-default
          weekend
      .description
        = gist.description
